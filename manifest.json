{
	"name": "heatmap",
	"description": "Page heat-map extension creates for university project",
	"version": "1.0",
	"manifest_version":2,
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"browser_action": {
		"default_title": "heatmap",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": [
		"tabs",
		"identity",
		"notifications",
		"http://*/",
		"https://*/"
	]
}